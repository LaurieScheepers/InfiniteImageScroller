<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Cabin is used for headings and sub-headings -->

    <!-- regular -->
    <font
        app:font="@font/cabin_regular"
        app:fontStyle="normal"
        app:fontWeight="400"

        android:font="@font/cabin_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        tools:ignore="UnusedAttribute" />

    <!-- italic -->
    <font
        app:font="@font/cabin_italic"
        app:fontStyle="italic"
        app:fontWeight="400"

        android:font="@font/cabin_italic"
        android:fontStyle="italic"
        android:fontWeight="400"
        tools:ignore="UnusedAttribute" />

</font-family>