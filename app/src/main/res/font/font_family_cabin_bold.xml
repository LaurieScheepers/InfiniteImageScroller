<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Cabin is used for headings and sub-headings -->

    <!-- bold -->
    <font
        app:font="@font/cabin_bold"
        app:fontStyle="normal"
        app:fontWeight="700"

        android:font="@font/cabin_bold"
        android:fontStyle="normal"
        android:fontWeight="700"
        tools:ignore="UnusedAttribute" />

    <!-- bold italic -->
    <font
        app:font="@font/cabin_bolditalic"
        app:fontStyle="italic"
        app:fontWeight="700"

        android:font="@font/cabin_bolditalic"
        android:fontStyle="italic"
        android:fontWeight="700"
        tools:ignore="UnusedAttribute" />

</font-family>